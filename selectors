//Яндекс
$$('[data-zone-name="price"] > div > a > div > span > span:first-of-type') - цены
$$('[data-auto="product-title"]) - текст ссылки
$$('._3ur4W') - граммы
$$('[data-auto="product-snippet"] > div:nth-child(3) > div:nth-child(3) > span') - цена за штуку
//Сбермаркет
$$('[class^=ProductCardPrice_price]') - цена
$$('[class^=ProductCard_volume]') - вес
